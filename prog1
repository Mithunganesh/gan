import java.io.*;
import util.*;
class prog1
{
public static void mani(String args[])
{
System.out.println("Enter the Number:");
Scanner s=new Scanner(System.in);
int n=s.nextInt();
if(n>0){
System.out.println("positive number");
}else if(n<0)
{
System.out.println("negative number");
}
else
{
System.out.println("zero");
}
}
}
